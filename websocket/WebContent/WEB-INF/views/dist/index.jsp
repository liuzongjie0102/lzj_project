<%@ page contentType="text/html;charset=UTF-8" language="java" %>
<html>
  <head>
    <title>$Title$</title>
  </head>
  <body>
  <div id="app"></div>
  <%--<button onclick="sayMarco()">send</button>--%>
  <script type="text/javascript" src="js/build.js"></script>
  <%--<script src="https://cdn.bootcss.com/sockjs-client/1.1.4/sockjs.min.js"></script>--%>
  <%--<script>--%>

      <%--var url = 'http://localhost:9016/websocketDemo/webServer';--%>
      <%--var sock = new SockJS(url);      //打开WebSocket--%>

      <%--sock.onopen = function() {          //处理连接开启事件--%>
          <%--console.log('Opening');--%>
          <%--sock.send('Marco!');--%>
      <%--};--%>

      <%--sock.onmessage = function(e) {      //处理信息--%>
          <%--console.log('Received Message: ', e.data);--%>
          <%--setTimeout(function() {--%>
              <%--sayMarco()--%>
          <%--}, 2000);--%>
      <%--};--%>

      <%--sock.onclose = function() {         //处理连接关闭事件--%>
          <%--console.log('Closing');--%>
      <%--};--%>

      <%--function sayMarco() {               //发送信息函数--%>
          <%--console.log('Sending Marco!');--%>
          <%--sock.send('Marco!');--%>
      <%--}--%>
  <%--</script>--%>
  </body>
</html>
